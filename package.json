{
  "name": "amphora-rss",
  "version": "0.6.0",
  "description": "Convert JSON from Amphora into RSS",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --max-warnings 0 index.js index.test.js services",
    "jest": "jest",
    "test": "npm run lint && npm run jest -- --coverage --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nymag/amphora-rss.git"
  },
  "author": "New York Media",
  "license": "MIT",
  "dependencies": {
    "clay-log": "^1.2.1",
    "date-fns": "^1.29.0",
    "highland": "^2.11.1",
    "xml": "^1.0.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^4.16.0",
    "jest": "^23.5.0",
    "lodash": "^4.17.10",
    "sshpk": "^1.14.2"
  },
  "jest": {
    "clearMocks": true,
    "restoreMocks": true,
    "collectCoverageFrom": [
      "<rootDir>/index.js",
      "<rootDir>/services/**/*.js"
    ],
    "coverageReporters": [
      "lcov",
      "text-summary"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "setupFiles": [
      "<rootDir>/test/setupFile.js"
    ]
  }
}
